{
    "manifest_version": 3,
    "name": "Element Notifier",
    "version": "0.0.1",
    "description": "Monitors for the appearance of a specific html blocks inside chrome tabs.",
    "permissions": ["tabs", "notifications", "storage", "scripting"],
    "key": "",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": ["https://*/*", "file://*"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }